{
  "comments": false,
  "env": {
    "main": {
      "presets": [
        ["env", {
          "targets": {
            "node": 7
          }
        }],
        "stage-0"
      ]
    },
    "renderer": {
      "presets": [
        ["env", {
          "modules": false
        }],
        "stage-0"
      ]
    },
    "web": {
      "presets": [
        ["env", {
          "modules": false
        }],
        
        "stage-0"
      ]
    }
  },
  
  "plugins":[
    ["prismjs", {
        "languages": ["javascript", "css", "markup"],
        "plugins": ["line-numbers"],
        "theme": "COY",
        "css": true
    }],
    "transform-runtime"
  ] 
}